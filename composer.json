{
    "name": "algolia/magento2-tools",
    "description": "Tools for magento 2 developers",
    "keywords": ["php", "magento", "tools"],
    "license": "MIT",
    "authors": [
        {
            "name": "Algolia Team",
            "email": "contact@algolia.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "friendsofphp/php-cs-fixer": "^2.15",
        "phpstan/phpstan": "^0.11.16"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.packagist.org"
        }
    ],
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    },
    "bin": [
        "bin/magento2-lint",
        "bin/magento2-test",
        "bin/magento2-types"
    ]
}
